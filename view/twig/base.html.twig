<!doctype html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/public/css/main.css">
    <link rel="shortcut icon" href="/public/imgs/logo3.png" />
    <link rel="stylesheet" href="/public/font-awesome/css/font-awesome.min.css">
    <link rel="navbarStyleSheet" href="/navbar.css">
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="/public/css/materialize.min.css" media="screen,projection" />
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    {% block stylesheets %}{% endblock %}

    <title>Hospital Gutierrez - Index</title>
</head>
<body>

    <header>{% include 'navbar.html.twig' %}</header>

    <section class="content">
        {% block body %}{% endblock %}
    </section>

    {% include 'footer/footer.html.twig' %}

    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="/public/js/materialize.min.js"></script>
    <script type="text/javascript" src="/view/navbar.js"></script>
    {% block javascripts %}{% endblock %}

</body>

</html>
